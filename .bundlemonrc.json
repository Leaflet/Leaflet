{
  "baseDir": "dist",
  "defaultCompression": "gzip",
  "files": [
    {
      "maxSize": "40kB",
      "path": "leaflet.js"
    }, {
      "maxSize": "3.2kB",
      "path": "leaflet.css"
    }
  ],
  "reportOutput": [
    [
      "github",
      {
        "checkRun": true,
        "commitStatus": false,
        "prComment": false
      }
    ]
  ]
}
