{
    "name": "poligonosapp",
    "version": "1.7.2-with-typescript-and-snyk",
    "homepage": "https://bitbucket.org/poligonosapp/",
    "description": "leaflet fork with typescript and snyk",
    "author": "Fabio Rodrigues <https://www.facebook.com/kefaro>, Luis Mendes <machadoluismendes@gmail.com>",
    "devDependencies": {
        "@babel/core": "^7.14.5",
        "@babel/preset-env": "^7.14.5",
        "@babel/register": "^7.14.5",
        "@rollup/plugin-json": "^4.1.0",
        "@typescript-eslint/eslint-plugin": "^4.26.1",
        "@typescript-eslint/parser": "^4.26.1",
        "eslint": "^7.28.0",
        "eslint-config-mourner": "^2.0.1",
        "git-rev-sync": "^3.0.1",
        "happen": "~0.3.2",
        "karma": "^6.3.3",
        "karma-chrome-launcher": "^3.1.0",
        "karma-edge-launcher": "^0.4.2",
        "karma-expect": "^1.1.3",
        "karma-firefox-launcher": "^2.1.0",
        "karma-ie-launcher": "^1.0.0",
        "karma-mocha": "^2.0.1",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-rollup-preprocessor": "^7.0.7",
        "karma-safari-launcher": "~1.0.0",
        "karma-sinon": "^1.0.5",
        "leafdoc": "^2.2.1",
        "mocha": "^8.4.0",
        "phantomjs-prebuilt": "^2.1.16",
        "prosthetic-hand": "^1.3.1",
        "rollup": "^2.51.2",
        "rollup-plugin-git-version": "^0.3.1",
        "rollup-plugin-json": "^4.0.0",
        "semantic-release": "^17.4.3",
        "sinon": "^7.5.0",
        "ssri": "^8.0.1",
        "tslint": "^6.1.3",
        "typescript": "^4.3.4",
        "uglify-js": "^3.12.6"
    },
    "style": "dist/leaflet.css",
    "files": [
        "dist",
        "src",
        "!dist/leaflet.zip"
    ],
    "main": "server.ts",
    "scripts": {
        "start": "node ./src/Server.ts",
        "docs": "node ./build/docs.ts",
        "pretest": "npm run lint",
        "test": "npm run test-nolint",
        "test-nolint": "karma start ./spec/karma.conf.ts",
        "build": "npm run rollup && npm run uglify",
        "release": "./build/publish.sh",
        "lint": "eslint src spec/suites docs/docs/tsc",
        "lintfix": "npm run lint -- --fix",
        "rollup": "rollup -c build/rollup-config.ts",
        "watch": "rollup -w -c build/rollup-watch-config.ts",
        "uglify": "uglifyjs dist/leaflet-src.ts -c -m -o dist/leaflet.ts --source-map filename=dist/leaflet.ts.map --source-map content=dist/leaflet-src.ts.map --source-map url=leaflet.ts.map --comments",
        "integrity": "node ./build/integrity.ts"
    },
    "eslintConfig": {
        "root": true,
        "globals": {
            "L": true
        },
        "env": {
            "commonjs": true,
            "amd": true,
            "node": true
        },
        "extends": "mourner",
        "parserOptions": {
            "ecmaVersion": 6,
            "sourceType": "module"
        },
        "rules": {
            "linebreak-style": [
                0,
                "unix"
            ],
            "no-mixed-spaces-and-tabs": [
                2,
                "smart-tabs"
            ],
            "indent": [
                2,
                "tab",
                {
                    "VariableDeclarator": 0,
                    "flatTernaryExpressions": true
                }
            ],
            "curly": 2,
            "spaced-comment": 2,
            "strict": 0,
            "wrap-iife": 0,
            "key-spacing": 0,
            "consistent-return": 0,
            "no-unused-expressions": [
                "error",
                {
                    "allowShortCircuit": true
                }
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:poligonosapp/Leaflet.git"
    },
    "keywords": [
        "gis",
        "map",
        "typescript",
        "patch",
        "port"
    ],
    "license": "BSD-2-Clause",
    "dependencies": {
        "@angular/compiler": "^11.2.14",
        "@angular/compiler-cli": "^11.0.0",
        "@types/eslint": "^7.2.13",
        "@typescript-eslint/types": "^4.27.0",
        "ansi-regex": "^6.0.1",
        "core-js": "^3.14.0",
        "css-what": "^5.0.1",
        "express": "^4.17.1",
        "highlight.js": "^11.0.1",
        "iconv": "^3.0.0",
        "iconv-lite-loader": "^1.0.0",
        "normalize-url": "6.0.1",
        "regenerator-runtime": "^0.13.7",
        "snyk": "^1.634.0",
        "tslib": "^2.0.0"
    },
    "peerDependencies": {
        "rollup": "^1.20.0 || ^2.0.0 || ^2.30.0",
        "tslib": "^2.0.0",
        "typescript": "^4.3.2"
    },
    "engines": {
        "node": "14.17.1",
        "npm": "6.14.13"
    }
}
