# EditorConfig
# http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.js]
indent_style = tab
tab_width = 4

[{package.json,.travis.yml}]
indent_style = space
indent_size = 2
