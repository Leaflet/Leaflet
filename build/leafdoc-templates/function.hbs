<table><thead>
	<tr>
		<th>Function</th>
		<th>Returns</th>
		<th>Description</th>
	</tr>
	</thead><tbody>
	{{#each documentables}}
	<tr id='{{id}}'>
		<td><code><b>{{name}}</b>(
		{{~#each params~}}
			{{#if type}}<nobr>&lt;{{{type type}}}&gt;</nobr> {{/if}}<i>{{name}}</i>
			{{~#unless @last}}, {{/unless}}{{/each~}}
		)</code></td>
		<td><code>{{{type type}}}</code></td>
		<td>{{{markdown comments}}}</td>
	</tr>
	{{/each}}
</tbody></table>